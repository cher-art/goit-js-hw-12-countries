{{#each this}}
<h1 class="title">{{name}}</h1>
    <div class="country">
      <div class="country-box__container">
        <ul>
          <li class="form">Capital: {{capital}}</li>
          <li class="form">Population: {{population}}</li>
          <li class="form">
            Languages:
            <ul>
            {{#each languages}}
              <li class="form-item">{{this.name}}</li>
              {{/each}}
            </ul>
          </li>
        </ul>
        <div class="country-box__img">
          <img src="{{flag}}"  alt="{{name}}" width="200"></img>
        </div>
      </div>
    </div>
{{/each}}